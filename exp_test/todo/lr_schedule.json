{
    "experiment": "GraphMerge",
    "group":  "percentile", 
    "move":false,
    "model":  "Sage",
    "log": true,

    "run_params":{
    "n_epochs":    20,
    "n_trials": 2,
    "batch_size": 128,
    "val_epoch":   2,
    "loss_func": "L2",
    "metrics": "scatter",
    "performance_plot": "SAM_base",
    "shuffle":true,
    "save":true,
    "seed":true
},
"learn_params":{
    "learning_rate":   3e-3,
    "schedule":   "warmup_expcos",
    "g_up":   2,
    "g_down":   0.95,
    "warmup": 3

},

    "hyper_params": {
        "hidden_channels":   32,  
        "conv_layers":   3,
        "conv_activation": "relu",
        "decode_layers":   1,
        "layernorm":      true
    },

   "data_params":{ 
    "case": "test_all_smass",
    "split": 0.8
}
}