{
    "experiment": "GraphMerge",
    "group":  "test_gpu", 
    "move":false,
    "model":  "Sage",
    "log": true,

    "run_params":{
    "n_epochs":    1000,
    "n_trials": 2,
    "batch_size": 512,
    "val_epoch":   2,
    "early_stopping": true,
    "patience": 10,
    "l1_lambda":1e-3,
    "l2_lambda":1e-3,
    "loss_func": "L2",
    "metrics": "scatter",
    "performance_plot": "SAM_base",
    "shuffle":true,
    "save":true,
    "seed":true,
    "num_workers": 4
},
"learn_params":{
    "learning_rate":   3e-3,
    "schedule":   "warmup_exp",
    "g_up":1,
    "g_down":0.95,
    "warmup":100,
    "period":5, 
    "eta_min":1e-5
},

    "hyper_params": {
        "hidden_channels":   64,  
        "conv_layers":   3,
        "conv_activation": "relu",
        "decode_layers":   1,
        "layernorm":      true
    },

   "data_params":{ 
    "case": "vlarge_redshift_50_all",
    "split": 0.8
}
}