{
    "experiment": "GraphMerge",
    "group":  "test_sweep_warmup_exp", 
    "move":true,
    "model":  "Sage",
    "log": true,

    "run_params":{
    "n_epochs":    1000,
    "n_trials": 1,
    "batch_size": 128,
    "val_epoch":   2,
    "early_stopping": true,
    "patience": 5,
    "loss_func": "L2",
    "metrics": "scatter",
    "performance_plot": "SAM_base",
    "shuffle":true,
    "save":true,
    "seed":true
},
"learn_params":{
    "learning_rate":   3e-3,
    "schedule":   "constant",
    "g_up":1,
    "g_down":0.95,
    "warmup":100,
    "period":5, 
    "eta_min":1e-5
},

    "hyper_params": {
        "hidden_channels":   32,  
        "conv_layers":   3,
        "conv_activation": "relu",
        "decode_layers":   1,
        "layernorm":      true
    },

   "data_params":{ 
    "case": "test_all_smass",
    "split": 0.8
}
}